<section class="selected-article">
    <div class="container">
        <div class="section-title d-flex align-items-center justify-content-center">
            <svg width="100" height="7" viewBox="0 0 100 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                <circle cx="3" cy="3.5" r="3" fill="#C1C1C1" />
                <path d="M6 3.5H100" stroke="#C1C1C1" />
            </svg>
            <h2>
                　精選文章　
            </h2>
            <svg width="100" height="7" viewBox="0 0 100 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                <circle cx="97" cy="3.5" r="3" transform="rotate(-180 97 3.5)" fill="#C1C1C1" />
                <path d="M94 3.5L-1.01328e-06 3.49999" stroke="#C1C1C1" />
            </svg>
        </div>
        <div class="swiper-container">
            <div class="swiper-wrapper">
                <% page.posts.each(function(post, i){ if(post.img==null){ return; } %>
                    <div class="swiper-slide">
                        <div class="selected-article-image">
                            <a href="<%- url_for(post.path) %>" class="selected-article-image">
                                <img src="<%= post.img %>" alt=""></a>
                        </div>
                        <h3>
                            <a href="<%- url_for(post.path) %>">
                                <%= post.title %>
                            </a>
                        </h3>
                        <ul class="selected-article-post-bar">
                            <li class="selected-article-post-tagbox">
                                <%- list_tags(post.categories, { show_count: false, class: 'selected-article-post-tag'
                                    }) %>
                            </li>
                            <li class="selected-article-post-date">
                                <time class="dt-published" datetime="<%= date_xml(post.date) %>"
                                    itemprop="datePublished">
                                    <%= date(post.date, "YYYY/MM/D" ) %>
                                </time>
                            </li>
                        </ul>
                    </div>
                    <% }) %>
            </div>
            <!-- Add Pagination -->
            <div class="swiper-pagination"></div>
        </div>
    </div>
</section>